
# .env.prod
# Production environment configuration

# API Settings
APP_ENV=prod
PROJECT_NAME="FastAPI Boilerplate"
SECRET_KEY="change-me-with-strong-secret-key"
SWAGGER_UI_ENABLED=false
BACKEND_CORS_ORIGINS=["https://yourdomain.com"]

# Database Settings
DATABASE_TYPE=postgres
POSTGRES_SERVER=postgres
POSTGRES_USER=postgres
POSTGRES_PASSWORD=secure-password-change-me
POSTGRES_DB=app
POSTGRES_PORT=5432

# Oracle Settings (if needed)
ORACLE_SERVER=oracle
ORACLE_USER=oracle
ORACLE_PASSWORD=secure-password-change-me
ORACLE_DB=app
ORACLE_PORT=1521

# Redis Settings
REDIS_SERVER=redis
REDIS_PORT=6379
REDIS_PASSWORD=secure-password-change-me
REDIS_DB=0

# MinIO Settings
MINIO_SERVER=minio
MINIO_PORT=9000
MINIO_ROOT_USER=minioadmin
MINIO_ROOT_PASSWORD=secure-password-change-me
MINIO_BUCKET_NAME=app-bucket

# Security Settings
ACCESS_TOKEN_EXPIRE_MINUTES=10080  # 7 days

# Rate Limit Settings
RATE_LIMIT_ENABLED=true
RATE_LIMIT_REQUESTS=60
RATE_LIMIT_WINDOW_SECONDS=60

# Cache Settings
CACHE_ENABLED=true
CACHE_EXPIRE_SECONDS=300  # 5 minutes

# Logging
LOG_LEVEL=INFO
SENTRY_DSN=your-sentry-dsn-here